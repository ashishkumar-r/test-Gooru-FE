          {{t 'common.question-compentency'}}
          <div>
            <span class="ok-buttons" {{action 'removePopup'}}>{{t 'common.yes'}}</span>
            <span class="ok-buttons" {{action 'onClosePullUp'}}>{{t 'common.no'}}</span>
          </div>
        </div>
      </div>
    </div>
  </div>
