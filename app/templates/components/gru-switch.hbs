<div class="switch">
  <a tabindex={{tabindex}} onkeypress={{action 'switchToggleButton'}} >
    <label for={{inputId}}>
      <input type="checkbox" id={{inputId}} title={{inputId}} aria-label="checkbox" disabled={{disabled}} data-toggle="toggle" data-on={{t optionA.label}} data-off={{t optionB.label}} data-width="61" data-height="34" aria-checked="false">
    </label>
  </a>
</div>
