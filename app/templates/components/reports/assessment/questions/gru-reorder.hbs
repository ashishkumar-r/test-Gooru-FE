<ul>
  {{#each answers as |answer index|}}
    <li>
      {{#if (eq answer.showCorrect true) }}
        <span class="answer-text">{{{gru-math-text text=answer.text}}}</span>
      {{else}}
        <span>{{{gru-math-text text=answer.selectedOrderText}}}</span>
      {{/if}}
    </li>
  {{/each}}
</ul>
